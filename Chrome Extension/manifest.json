{
  "name": "Zeepium",
  "version": "1.0",
  "description": "This chrome extension will run with Zeepium app to stream any zee5 content inside application.",
  "manifest_version": 2,
  "incognito": "split",
  "browser_action": {
    "default_icon": "icons/icon256.png",
    "default_popup": "index.html",
    "default_title": "Zeepium"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs",
    "activeTab"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  }
}
